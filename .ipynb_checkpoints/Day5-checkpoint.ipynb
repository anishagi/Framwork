    "#Take multiple numbers as input and print the sum\n",
    "A=[int(i) for i in input(\"Enter the numbers:\").split()]\n",
    "sum = 0\n",
    "for j in A:\n",
    "    sum += j\n",
    "print(sum)\n",
    "\n"
   ]
